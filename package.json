    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "better-sqlite3": "^11.8.1",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
