{
  "name": "task-manager-fullstack-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "build:server": "tsc --project tsconfig.server.json",
    "start:server": "node dist/backend/server.js",
    "ts:check": "tsc --noEmit",
    "server": "ts-node src/backend/server.ts",
    "docker": "docker compose down && docker compose up --build",
    "docker:clear": "docker system prune --all --volumes",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy && yarn prisma:generate",
    "prisma:migrate:dev": "prisma migrate dev --name init && yarn prisma:generate",
    "prisma:pull": "prisma db pull && yarn prisma:generate",
    "prisma:push": "prisma db push && yarn prisma:generate",
    "prisma:studio": "prisma studio",
    "cypress": "cypress open --e2e",
    "lint": "eslint \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "eslint \"src/**/*.+(ts|js|tsx)\" --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.1",
    "@mantine/core": "^7.17.0",
    "@mantine/hooks": "^7.17.0",
    "@prisma/client": "^6.4.1",
    "@tabler/icons-react": "^3.30.0",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "better-sqlite3": "^11.8.1",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "cypress": "^14.1.0",
    "express": "^4.21.2",
    "next": "^15.3.0-canary.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2",
    "@types/cypress": "^1.1.6",
    "@types/eslint-plugin-jsx-a11y": "^6",
    "@types/express": "^5.0.0",
    "@types/node": "^20.17.24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "prettier": "^3.5.1",
    "prisma": "^6.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "packageManager": "yarn@4.6.0+sha512.5383cc12567a95f1d668fbe762dfe0075c595b4bfff433be478dbbe24e05251a8e8c3eb992a986667c1d53b6c3a9c85b8398c35a960587fbd9fa3a0915406728"
}
