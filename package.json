{
  "name": "task-manager-fullstack-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "ts:compile": "tsc",
    "server": "npx ts-node src/backend/server.ts",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:pull": "npx prisma db pull && yarn run prisma:generate",
    "prisma:push": "npx prisma db push && yarn run prisma:generate",
    "prisma:migrate": "npx prisma migrate dev --name init && yarn run prisma:generate",
    "cypress": "npx cypress open --e2e",
    "lint": "eslint \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "eslint \"src/**/*.+(ts|js|tsx)\" --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write"
  },
  "dependencies": {
